@model IEnumerable<_2024_InstitutoEducativo.Models.Calificacion>

@{
    ViewData["Title"] = "Calificaciones";
}

<h1>Calificaciones</h1>

<
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NotaFinal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MateriaCursada.Cuatrimestre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MateriaCursada)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Profesor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Alumno)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NotaFinal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MateriaCursada.Cuatrimestre)
            </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MateriaCursada.Nombre)
                </td>
            <td>
                @Html.DisplayFor(modelItem => item.Profesor.NombreCompleto)
            </td>
            <td>
                    @Html.DisplayFor(modelItem => item.Alumno.NombreCompleto)
            </td>
            
        </tr>
}
    </tbody>
</table>
